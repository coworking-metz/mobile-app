{
  "expo": {
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#F9B000",
        "foregroundImage": "./src/assets/images/adaptive-icon.png"
      },
      "package": "fr.coworking_metz.mobile"
    },
    "assetBundlePatterns": ["**/*"],
    "experiments": {
      "tsconfigPaths": true
    },
    "extra": {
      "eas": {
        "projectId": "46b88b1b-3fc8-457c-b375-b33fc168a776"
      },
      "router": {
        "origin": false
      }
    },
    "icon": "./src/assets/images/icon.png",
    "ios": {
      "bundleIdentifier": "fr.coworking-metz.mobile",
      "supportsTablet": false,
      "privacyManifests": {
        "NSPrivacyAccessedAPITypes": [
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryFileTimestamp",
            "NSPrivacyAccessedAPITypeReasons": [
              "C617.1"
            ]
          },
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategorySystemBootTime",
            "NSPrivacyAccessedAPITypeReasons": [
              "35F9.1"
            ]
          },
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryDiskSpace",
            "NSPrivacyAccessedAPITypeReasons": [
              "E174.1"
            ]
          },
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryUserDefaults",
            "NSPrivacyAccessedAPITypeReasons": [
              "CA92.1"
            ]
          }
        ]
      }
    },
    "name": "Le Poulailler",
    "orientation": "portrait",
    "owner": "coworking-metz",
    "packagerOpts": {
      "assetExts": ["md"]
    },
    "plugins": [
      "@vonovak/react-native-theme-control",
      "expo-localization",
      "expo-router",
      "expo-secure-store",
      [
        "@sentry/react-native/expo",
        {
          "organization": "coworking-metz",
          "project": "mobile"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 23,
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0",
            "kotlinVersion": "1.8.10"
          },
          "ios": {
            "deploymentTarget": "14.0"
          }
        }
      ],
      [
        "expo-custom-assets",
        {
          "assetsPaths": ["./src/assets/rive"]
        }
      ]
    ],
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "scheme": "poulailler",
    "slug": "coworking-mobile",
    "splash": {
      "backgroundColor": "#EAB234",
      "image": "./src/assets/images/splash.png",
      "resizeMode": "contain"
    },
    "updates": {
      "url": "https://u.expo.dev/d4548eb7-404d-4a34-a484-8c0a1f01e0d6"
    },
    "userInterfaceStyle": "automatic",
    "version": "2024.9.2"
  }
}
